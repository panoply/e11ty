{
  "name": "@e11ty/eleventy-plugin-svg-sprite",
  "version": "0.0.1",
  "description": "11ty Plugin for generating SVG Sprites for icons",
  "module": "./dist/index.mjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    },
    "require": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/panoply/e11ty/plugins/eleventy-plugin-svg-sprite"
  },
  "bugs": {
    "url": "https://github.com/panoply/e11ty/issues"
  },
  "files": [
    "dist",
    "LICENSE",
    "readme.md"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup --minify",
    "postversion": "pnpm bump",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "prettier": "@sissel/prettier-config",
  "eslintConfig": {
    "extends": "@sissel/eslint-config",
    "ignorePatterns": [
      "dist"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "11ty.ts": "workspace:^",
    "@types/mergerino": "^0.4.5",
    "@types/svg-sprite": "^0.0.39",
    "mergerino": "^0.4.0",
    "type-fest": "^4.15.0"
  },
  "peerDependencies": {
    "@11ty/eleventy": "^2.0.1"
  },
  "dependencies": {
    "fast-glob": "^3.3.2",
    "svg-sprite": "^2.0.4",
    "vinyl": "^3.0.0"
  }
