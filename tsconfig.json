{
  "exclude": [
    "node_modules",
    "**/*.cjs",
    "**/*.mjs",
    "**/*.d.ts"
  ],
  "include": [
    "plugins/**/*.ts",
    "package/*.ts"
  ],
  "compilerOptions": {
    "outDir": "dist",
    "incremental": false,
    "strictNullChecks": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noEmit": true,
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "target": "ES6",
    "module": "CommonJS"
  }
}